.menu.menunav {
	background-color:#323a45;
}
.menunav .item {
	height: 84px;
	background-color:#323a45;
	font-size: 11px;
	color: #9f9a97;
	border: none;
	text-align: center;
}
.menunav .item:first-child {
	color: #fff;
	height: 48px;
	background: #2a2e37;
}
.menunav .out_icon {
	display: inline-block;
	width: 16px;
	height: 16px;
	line-height: 14px;
	border: 1px solid #fff;
	border-radius: 16px;
	text-align: center;
	color: #fff;
	margin-right: 5px;
}
.menunav .customerhead,.menunav .customerlabel {
	display: block;
	font-size: 28px;
}
.menunav .item.activated {
	color: #fff;
}
